Algoritmo "contaAte10semnome"

Var
   // Seção de Declarações das variáveis
   contador : Inteiro

Inicio
   // Seção de Comandos, procedimento, funções, operadores, etc...
   contador <- 0
   Enquanto (contador <= 10 ) faca
      EscrevaL(contador)

      contador <- contador + 1
   FimEnquanto


Fimalgoritmo