Algoritmo "contaAte0"

Var
   // Seção de Declarações das variáveis
   contador : Inteiro

Inicio
   // Seção de Comandos, procedimento, funções, operadores, etc...
   contador <- 10
   Enquanto (contador <= 0 ) faca
      EscrevaL(contador)

      contador <- contador - 1
   FimEnquanto
   EscrevaL("Terminei de contar")


Fimalgoritmo